{
  "name": "karet",
  "version": "1.1.0",
  "description": "Karet is a library that allows you to embed Kefir observables into React VDOM",
  "main": "lib/karet.js",
  "scripts": {
    "dist": "babel src --source-maps inline --out-dir lib",
    "lint": "eslint src test",
    "prepublish": "npm run lint && npm run test && npm run dist",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "test": "nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calmm-js/karet.git"
  },
  "keywords": [
    "jsx",
    "kefir",
    "observables",
    "react",
    "reactive",
    "vdom"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/calmm-js/karet/issues"
  },
  "homepage": "https://github.com/calmm-js/karet#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "codecov": "^2.1.0",
    "eslint": "^3.19.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-react": "^6.10.3",
    "kefir": "^3.7.1",
    "mocha": "^3.2.0",
    "nyc": "^10.2.0",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.3"
  },
  "dependencies": {
    "infestines": "^0.4.1",
    "react": "^15.0.1"
  },
  "peerDependencies": {
    "kefir": "^3.7.0"
  }
}
